package com.spring.service;

import java.util.List;

import com.spring.vo.BoardVO;

public interface BoardService {
	public List<BoardVO> getAllBoard(int start, int count) throws Exception;

	public int getAllCount() throws Exception;

	public BoardVO getOneBoard(int num);

	public int getRef();

	public void insertBoard(BoardVO bean, int ref);

	public void setReLevel(int ref, int re_level);

	public void reWriteBoard(BoardVO bean);
}
